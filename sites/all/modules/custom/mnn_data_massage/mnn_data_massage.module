function mnn_data_massage_update_premiere_date_and_channels() {
  $start = strtotime('now');
  $num = filter_input(INPUT_GET, 'num') ? filter_input(INPUT_GET, 'num') : 1;

  $max_nid =
    variable_get('mnn_data_massage_premiere_date_and_channel_last_nid',
		 100000000);

  $sql =
    "SELECT nid from node ".
    "WHERE type='cm_show' AND nid < $max_nid ".
    "ORDER BY nid desc LIMIT $num";
  dsm($num, 'num');
  dsm($max_nid, 'mid');
  dsm($sql, 'sql');

  $results = db_query($sql);

  while ($result = $results->fetchObject()) {
    $end = strtotime('now');
    dsm("loop start " . mnn_data_massage_hms($end-$start));

    $show = node_load($result->nid);
    $end = strtotime('now');
    dsm("pre channel " . mnn_data_massage_hms($end-$start));

    $show->field_show_channels[LANGUAGE_NONE][0]['value'] =
      mnn_show_get_channels($show);

    $end = strtotime('now');
    dsm("pre premiere " . mnn_data_massage_hms($end-$start));

    $date = mnn_show_get_premiere_date($show);

    $end = strtotime('now');
    dsm("post premiere " . mnn_data_massage_hms($end-$start));
    
    if ($date) {
      $show->field_show_premiere_date[LANGUAGE_NONE][0]['value'] =
	$date;
    }
    $end = strtotime('now');
    dsm("pre show " . mnn_data_massage_hms($end-$start));
    field_attach_update('node', $show);

    $end = strtotime('now');
    dsm("post show " . mnn_data_massage_hms($end-$start));

    //drupal_set_message(l($show->title, "node/".$show->nid) . ' ' .
    //		       $date);
    variable_set('mnn_data_massage_premiere_date_and_channel_last_nid',
		 $show->nid);
    $end = strtotime('now');
    dsm("post variable set " . mnn_data_massage_hms($end-$start));

  }
  
  $end = strtotime('now');
  dsm("took " . mnn_data_massage_hms($end-$start));
}
function mnn_data_massage_hms($seconds) {
    $hours = floor($seconds / 3600);
    $minutes = floor(($seconds / 60) % 60);
    $seconds = $seconds % 60;
    return "$hours:$minutes:$seconds";
}
function mnn_data_massage_log_query($sql, $args) {
  $ms = str_replace('{', '', $sql);
  $ms = str_replace('}', '', $ms);
  foreach ($args as $i=>$j) {
    $ms = str_replace($i, $j, $ms);
  }
  dsm($ms);
  dsm($args);
}
