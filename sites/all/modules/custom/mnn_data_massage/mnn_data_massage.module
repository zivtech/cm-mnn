function mnn_data_massage_producer_role_update() {
  $results = $_SESSION['mdm_user_roles_to_add'];
  if (!$results) {
    drupal_goto('admin/mnn_data_massage/producer_role_check');
  }
  $counter = 0;

  foreach ($results as $uid=>$role_string) {
    array_shift($_SESSION['mdm_user_roles_to_add']);
    $roles = explode(',',$role_string);

    drupal_set_message("Updating ".l($uid, "user/$uid/edit").
		       " with  $role_string");
    foreach ($roles as $role_name) {
      if ($role = user_role_load_by_name($role_name)) {
	user_multiple_role_edit(array($uid), 'add_role', $role->rid);
      }
      $counter ++;
    }

    dsm("Note, limiting to 10 at a time, remove when ready to run for real");
    if ($counter >= 10) {
      break;
    }
  }

  dsm("Updated $counter users"); 
  drupal_goto('admin/mnn_data_massage/producer_role_check');


}

function mnn_data_massage_items_missing_status_update() {

  $sql =
   "
SELECT node.title AS node_title, node.nid AS nid,
field_data_field_equipment_item_status.field_equipment_item_status_tid as status_tid,
node.type AS node_type,
reservations_node_type.reservations_active_status AS active_status,
reservations_reservation_item_node.reservations_default_availability AS default_avai,
reservations_node_type.reservations_type_setting AS type_setting,
field_data_field_inventory_number.field_inventory_number_value AS inventory_number
FROM
node node
LEFT JOIN field_data_field_equipment_item_status field_data_field_equipment_item_status
ON node.nid = field_data_field_equipment_item_status.entity_id
AND (field_data_field_equipment_item_status.entity_type = 'node' AND field_data_field_equipment_item_status.deleted = '0')

INNER JOIN reservations_node_type reservations_node_type ON node.type = reservations_node_type.type

LEFT JOIN reservations_reservation_item_node reservations_reservation_item_node ON node.vid = reservations_reservation_item_node.vid

LEFT JOIN field_data_field_inventory_number field_data_field_inventory_number
ON node.nid = field_data_field_inventory_number.entity_id AND (field_data_field_inventory_number.entity_type = 'node'
AND field_data_field_inventory_number.deleted = '0')

WHERE (( (node.status = '1') AND (node.type NOT IN  ('civicrm_multiday_event', 'crew_connect_application', 'crew_connect_request', 'document',
'equipment_non_reservable_', 'equipment_template', 'facility_non_reservable_', 'facility_template', 'help', 'cm_project', 'reservations_reservation',
'cm_show', 'simple_page', 'webform')) AND (field_data_field_equipment_item_status.field_equipment_item_status_tid IS NULL ) ))

   ";

  $results = db_query($sql);
  $counter = $active_count = $inactive_count = 0;
  while ($item = $results->fetchObject()) {
    $counter ++;
    $node = node_load($item->nid);

    if ($item->active_status == 1) {
      $node->field_equipment_item_status[LANGUAGE_NONE][0]['tid'] = 811;
	$active_count ++;
    }
    else {
      $node->field_equipment_item_status[LANGUAGE_NONE][0]['tid'] = 817;
    }
    
    node_save($node);
  }
  dsm("updated $counter items");
  dsm("updated $active_count items");

}

function mnn_data_massage_delete_program_import_shows_and_airings() {
  dsm("hi mom");
  $nids = array(963612, 963657);
  dsm(date('r'), 'start');
  foreach ($nids as $nid) {
    $sql =
      "SELECT cm_airing.airing_id " .
      "FROM cm_airing cm_airing " .
      "INNER JOIN field_data_field_uploader_airingref afield  " .
      "ON cm_airing.airing_id = afield.field_uploader_airingref_target_id  ".
      "AND (afield.entity_type = 'node' AND afield.deleted = '0') " .
      "INNER JOIN node n ON afield.entity_id = n.nid " .
      "WHERE n.nid = $nid";

    dsm($sql);

    $results  = db_query($sql);
    $counter = 0;
    while ($result = $results->fetchObject()) {
      entity_delete('airing', $result->airing_id);
      $counter ++;
    }
    dsm($counter, 'airing counter');

    $sql =
      "SELECT sfield.field_uploader_showref_target_id as show_id " .
      "FROM field_data_field_uploader_showref sfield  " .
      "INNER JOIN node n on n.nid = sfield.field_uploader_showref_target_id ".
      "WHERE sfield.entity_type = 'node' AND sfield.deleted = '0' " .
      "AND sfield.entity_id = $nid";

    dsm($sql);

    $results  = db_query($sql);
    $counter = 0;
    while ($result = $results->fetchObject()) {
      $delete_id = $result->show_id;
      node_delete($delete_id);
      $counter ++;
    }
    
    dsm($counter, 'show counter');

  }
  dsm(date('r'), 'end');
}
function mnn_data_massage_update_project_internet_archive_opt_out() {
  //see formats listed below in arrays
  $opt_out_tids = array(//all tids from inactive taxonomy
			784,785,1151,786,1150,787,
			//tids from project type
			763,
			//intervals
			1617,
			);

  $formats = array(
		   766 => "1/2",
		   1142 => "Archival",
		   767 => "DV",
		   768 => "DVD",
		   769 => "LIVE",
		   770 => "MPEG2",
		   1062 => "Other",
		   );
  
  $intervals = array(
		     780 => "Once",
		     775 => "Monthly",
		     774 => "Every Two Weeks",
		     781 => "Weekly",
		     771 => "Every Weekday",
		     1617 => "Promo",
		     );
  
  $inactive_status = array(
			   784 => "Cancelled",
			   785 => "Completed",
			   1151 => "Inactive",
			   786 => "Incomplete",
			   1150 => "Pilot",
			   787 => "Withdrawn",
			   );
  
  $runtimes = array(
		    850 => "28 mins",
		    851 => "58 mins",
		    852 => "88 mins",
		    853 => "118 mins",
		    854 => "Other",
		    );
  
  $project_types = array(
			 764 => "Archival",
			 762 => "Fill",
			 763 => "Promo",
			 765 => "Rental",
			 1 => "Series",
			 2 => "Special",
			 );

  $available_tids = $formats + $intervals + $inactive_status +
    $runtimes + $project_types;

  $limit = 20;  

  $sql = "SELECT nid FROM node WHERE type='cm_project' ORDER BY nid DESC ";
  
  if ($limit) {
    $sql .= " LIMIT $limit ";
  }

  $results = db_query($sql);

  while ($result = $results->fetchObject()) {
    $pnid = $result->nid;
    $project = node_load($pnid);
    dsm('-------------------');
    dsm($project->title, 'Project Title:');

    $field = "field_internet_archive_opt_out";
    $opt_out = mnn_internet_archive_get_single_field_value($project, $field);

    if ($opt_out == 1) {
      dsm("already opted out");
      continue;
    }
    if ($project->status != 1) {
      $opt_out = 1;
    }
    else {
      $fields = array(
		      "field_format",
		      "field_interval",
		      "field_reason_not_active",
		      "field_runtime",
		      "taxonomy_cm_project_types",
		      );
      $tids = array();
      foreach($fields as $field) {
	$tid = 	  mnn_internet_archive_get_single_field_value($project, $field, 'tid');
	if ($tid) {
	  $tids[] = $tid;
	}
      }

      dsm(implode(',',$tids), 'project tids');
      $intersect = array_intersect($tids, $opt_out_tids);

      if (count($intersect) > 0) {
	$opt_out = 1;
	foreach($intersect as $tid) {
	  dsm('opt out because: ' . $available_tids[$tid]);
	}
      }
    }

    if ($opt_out == 1) {
     
      $project->field_internet_archive_opt_out[LANGUAGE_NONE][0]
	['value'] = 1;
      dsm($project, 'proj');
      node_save($project);
    }
  
    
  }
}
function mnn_data_massage_prune_replication_history() {
  dsm(date('h:i:s'));
  $sql =
    "SELECT entity_id as nid, field_replication_history_value as history
     FROM field_data_field_replication_history 
     ORDER by entity_id asc";

  $results = db_query($sql);
  $counter = 0;
  while ($result = $results->fetchObject()) {
    if ($result->history) {
      $history = unserialize($result->history);
      $new_history = array();
      foreach($history as $quarter=>$airings) {
	$dates = explode('--', $quarter);
	$year = date('Y', strtotime($dates[1]));
	$allowed_year = date('Y',strtotime('now')) - 1;

	if ($year < $allowed_year) {
	  continue;
	}
	$new_history[$quarter] = $airings;
      }

      $node = node_load($result->nid);

      //dsm($node->field_replication_history[LANGUAGE_NONE][0], 'node1');

      $node->field_replication_history[LANGUAGE_NONE][0] =
	array('value' => serialize($new_history));
      
      //dsm($node->field_replication_history[LANGUAGE_NONE][0], 'node2');
      node_save($node);
    }
    $counter ++;
  }
  dsm($counter ." results");
  dsm(date('h:i:s'));
}

